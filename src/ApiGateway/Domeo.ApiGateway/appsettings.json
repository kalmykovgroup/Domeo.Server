{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "AllowedOrigins": []
  },
  "Jwt": {
    "Secret": "your-super-secret-key-at-least-32-characters-long",
    "Issuer": "domeo",
    "Audience": "domeo-api"
  },
  "Seq": {
    "ServerUrl": "http://localhost:5341"
  },
  "ReverseProxy": {
    "Routes": {
      "auth-route": {
        "ClusterId": "auth-cluster",
        "Match": {
          "Path": "/api/auth/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api" }
        ]
      },
      "users-route": {
        "ClusterId": "users-cluster",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/api/users/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api" }
        ]
      },
      "clients-route": {
        "ClusterId": "users-cluster",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/api/clients/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api" }
        ]
      },
      "projects-route": {
        "ClusterId": "projects-cluster",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/api/projects/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api" }
        ]
      },
      "rooms-route": {
        "ClusterId": "projects-cluster",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/api/rooms/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api" }
        ]
      },
      "edges-route": {
        "ClusterId": "projects-cluster",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/api/edges/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api" }
        ]
      },
      "cabinets-route": {
        "ClusterId": "cabinets-cluster",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/api/cabinets/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api" }
        ]
      },
      "materials-route": {
        "ClusterId": "catalog-cluster",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/api/materials/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api" }
        ]
      },
      "material-categories-route": {
        "ClusterId": "catalog-cluster",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/api/material-categories/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api" }
        ]
      },
      "module-types-route": {
        "ClusterId": "catalog-cluster",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/api/module-types/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api" }
        ]
      },
      "module-categories-route": {
        "ClusterId": "catalog-cluster",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/api/module-categories/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api" }
        ]
      },
      "hardware-route": {
        "ClusterId": "catalog-cluster",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/api/hardware/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api" }
        ]
      },
      "suppliers-route": {
        "ClusterId": "catalog-cluster",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/api/suppliers/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api" }
        ]
      }
    },
    "Clusters": {
      "auth-cluster": {
        "Destinations": {
          "auth-api": {
            "Address": "http://localhost:5001"
          }
        }
      },
      "users-cluster": {
        "Destinations": {
          "users-api": {
            "Address": "http://localhost:5002"
          }
        }
      },
      "projects-cluster": {
        "Destinations": {
          "projects-api": {
            "Address": "http://localhost:5003"
          }
        }
      },
      "cabinets-cluster": {
        "Destinations": {
          "cabinets-api": {
            "Address": "http://localhost:5004"
          }
        }
      },
      "catalog-cluster": {
        "Destinations": {
          "catalog-api": {
            "Address": "http://localhost:5005"
          }
        }
      }
    }
  }
}
