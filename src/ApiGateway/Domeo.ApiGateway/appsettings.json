{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:3000",
      "http://localhost:5173",
      "http://localhost:4200"
    ]
  },
  "AuthCenter": {
    "Authority": "http://localhost:5100",
    "Audience": "domeo"
  },
  "Seq": {
    "ServerUrl": "http://localhost:5341"
  },
  "ReverseProxy": {
    "Routes": {
      "auth-route": {
        "ClusterId": "auth-cluster",
        "Match": {
          "Path": "/api/auth/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api" }
        ]
      },
      "clients-route": {
        "ClusterId": "clients-cluster",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/api/clients/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api" }
        ]
      },
      "audit-route": {
        "ClusterId": "audit-cluster",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/api/audit/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api" }
        ]
      },
      "projects-route": {
        "ClusterId": "projects-cluster",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/api/projects/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api" }
        ]
      },
      "rooms-route": {
        "ClusterId": "projects-cluster",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/api/rooms/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api" }
        ]
      },
      "edges-route": {
        "ClusterId": "projects-cluster",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/api/edges/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api" }
        ]
      },
      "cabinets-route": {
        "ClusterId": "projects-cluster",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/api/cabinets/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api" }
        ]
      },
      "modules-route": {
        "ClusterId": "modules-cluster",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/api/modules/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api/modules" }
        ]
      },
      "materials-route": {
        "ClusterId": "materials-cluster",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/api/materials/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api/materials" }
        ]
      }
    },
    "Clusters": {
      "auth-cluster": {
        "Destinations": {
          "auth-api": {
            "Address": "http://localhost:5001"
          }
        }
      },
      "clients-cluster": {
        "Destinations": {
          "clients-api": {
            "Address": "http://localhost:5006"
          }
        }
      },
      "audit-cluster": {
        "Destinations": {
          "audit-api": {
            "Address": "http://localhost:5007"
          }
        }
      },
      "projects-cluster": {
        "Destinations": {
          "projects-api": {
            "Address": "http://localhost:5003"
          }
        }
      },
      "modules-cluster": {
        "Destinations": {
          "modules-api": {
            "Address": "http://localhost:5008"
          }
        }
      },
      "materials-cluster": {
        "Destinations": {
          "materials-api": {
            "Address": "http://localhost:5009"
          }
        }
      }
    }
  }
}
